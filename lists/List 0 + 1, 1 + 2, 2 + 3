#Creates a list from the specified numbers, consisting of the sums of adjacent numbers (0 and 1, 1 and 2, 2 and 3)

n = int(input())
list_1 = []
list_2 = []

for i in range(n):
    digit = int(input())
    list_1.append(digit)

for k in range(len(list_1) - 1):
    list_2.append(list_1[k] + list_1[k+1])

print(list_2)
